{
  "compilerOptions": {
    "target": "es2020",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "rootDir": "src",
    "module": "commonjs",
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": false,
    "declaration": false,
    "outDir": "lib",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "paths": {
      "@polkadot/api/augment": ["./d9/interfaces/augment-api.ts"],
      "@polkadot/types/augment": ["./d9/interfaces/augment-types.ts"]
    },
  },
  "include": ["src"],
  "exclude": [
    "node_modules"
  ]
}
